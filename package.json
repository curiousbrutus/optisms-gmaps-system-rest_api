{
  "name": "sms-review-flow",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "db:migrate": "node src/db/migrate.js",
    "test": "NODE_ENV=test vitest",
    "test:coverage": "NODE_ENV=test vitest --coverage",
    "test:ui": "NODE_ENV=test vitest --ui",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "dependencies": {
    "better-sqlite3": "9.6.0",
    "body-parser": "1.20.2",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-rate-limit": "7.4.0",
    "nanoid": "5.0.7",
    "node-fetch": "3.3.2",
    "pino": "9.4.0",
    "pino-http": "10.3.0"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.3.3",
    "supertest": "^7.1.4",
    "vitest": "^2.1.9"
  }
}
